<div class="all-users">
  {{#each user in model}}
    <div class="a-user">
      {{#link-to 'user' user}}
        <h2>{{user.firstName}}
        {{user.lastName}}</h2>
      {{/link-to}}
      <button {{action 'deleteUser' user.id}}>Delete User</button>
      {{#link-to 'user.edit' user}}
        <button>Edit User</button>
      {{/link-to}}
    </div>
  {{/each}}
</div>

{{outlet}}
